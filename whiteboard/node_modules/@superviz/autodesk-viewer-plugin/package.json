{
  "name": "@superviz/autodesk-viewer-plugin",
  "version": "1.16.0",
  "main": "dist/index.js",
  "license": "BSD-2-Clause",
  "description": "AutoDesk Viewer Plugin for SuperViz SDK",
  "publishConfig": {
    "access": "public",
    "scope": "@superviz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SuperViz/autodesk-viewer-plugin.git"
  },
  "scripts": {
    "prepare": "husky install",
    "build": "node ./.esbuild/build.js",
    "postbuild": "tsc --emitDeclarationOnly --declaration",
    "watch": "concurrently -n code,types \"yarn watch:code\" \"yarn watch:types\"",
    "watch:code": "node ./.esbuild/watch.js",
    "watch:types": "tsc --watch",
    "semantic-release": "semantic-release",
    "commit": "git-cz"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@types/debug": "^4.1.8",
    "@types/forge-viewer": "^7.69.5",
    "@types/lodash": "^4.14.197",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "concurrently": "^7.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.0.1",
    "esbuild": "^0.14.48",
    "eslint": "^8.26.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.2",
    "husky": "^8.0.3",
    "semantic-release": "19.0.5",
    "tsc": "^2.0.4",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "@superviz/sdk": "5.9.0-beta.1",
    "debug": "^4.3.4",
    "lodash": "^4.17.21",
    "three": "0.139.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
