{
  "name": "@superviz/react-sdk",
  "private": false,
  "version": "0.0.1-beta",
  "type": "module",
  "scripts": {
    "watch:demo": "vite --host --port 3000",
    "watch:build": "tsc && vite build --watch",
    "build": "tsc && vite build && cp package.json dist/package.json && cp README.md dist/README.md",
    "prepare": "husky install",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint --fix 'src/**/*.{jsx,ts,tsx}'",
    "format": "prettier --write src//**/*.{ts,tsx,css} --config ./.prettierrc",
    "semantic-release": "semantic-release",
    "commit": "git-cz"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "yarn eslint",
      "yarn format"
    ]
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/superviz-sdk-react.es.js",
      "require": "./dist/superviz-sdk-react.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "main": "./dist/superviz-sdk-react.umd.js",
  "module": "./dist/superviz-sdk-react.es.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public",
    "scope": "@superviz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SuperViz/sdk-react.git"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@superviz/autodesk-viewer-plugin": "1.16.0",
    "@superviz/matterport-plugin": "0.19.0",
    "@superviz/threejs-plugin": "0.22.0",
    "@superviz/sdk": "6.0.0-lab.3",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "@types/forge-viewer": "^7.69.9",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/three": "^0.160.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitejs/plugin-react": "^4.2.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1",
    "react-hooks": "^1.0.1",
    "semantic-release": "^22.0.5",
    "semantic-release-version-file": "^1.0.2",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-dts": "^3.6.4",
    "vite-plugin-linter": "^2.0.7",
    "vite-tsconfig-paths": "^4.2.2"
  }
}
