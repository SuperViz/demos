{
  "name": "@superviz/socket-client",
  "version": "1.2.2",
  "description": "SuperViz Socket Client",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "node ./.esbuild/build.js",
    "postbuild": "./node_modules/typescript/bin/tsc --emitDeclarationOnly --declaration",
    "watch": "concurrently -n code,types \"yarn watch:code\" \"yarn watch:types\"",
    "watch:code": "node ./.esbuild/watch.js",
    "watch:types": "./node_modules/typescript/bin/tsc --watch --outDir dist",
    "semantic-release": "semantic-release",
    "commit": "git-cz"
  },
  "publishConfig": {
    "access": "public",
    "scope": "@superviz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SuperViz/sv-socket-client.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SuperViz/sv-socket-client/issues"
  },
  "homepage": "https://github.com/SuperViz/sv-socket-client#readme",
  "devDependencies": {
    "@commitlint/cli": "^18.0.0",
    "@commitlint/config-conventional": "^18.0.0",
    "@esm-bundle/chai": "^4.3.4-fix.0",
    "@types/debug": "^4.1.10",
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.8",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "concurrently": "^7.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.0.1",
    "esbuild": "^0.14.48",
    "eslint": "^8.52.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "rimraf": "^5.0.5",
    "semantic-release": "^22.0.5",
    "ts-jest": "^29.1.1",
    "tsc": "^2.0.4",
    "typescript": "^5.2.2",
    "yargs": "^17.7.2"
  },
  "dependencies": {
    "@reactivex/rxjs": "^6.6.7",
    "debug": "^4.3.4",
    "lodash": "^4.17.21",
    "rxjs": "^7.8.1",
    "semantic-release-version-file": "^1.0.2",
    "socket.io-client": "^4.7.4",
    "zod": "^3.22.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
